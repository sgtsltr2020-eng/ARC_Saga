{
    "// ==========================================": "CURSOR AI CORE CONFIGURATION",
    "// ==========================================": "",
  
    "cursor.ai.enabled": true,
    "cursor.ai.model": "claude-4-5",
    "cursor.ai.temperature": 0.3,
    "cursor.ai.maxTokens": 8000,
    "cursor.ai.streaming": true,
    "cursor.ai.includeMarkdown": true,
  
    "cursor.ai.codeGeneration.style": "defensive",
    "cursor.ai.codeGeneration.includeTests": true,
    "cursor.ai.codeGeneration.includeDocumentation": true,
    "cursor.ai.codeGeneration.includeErrorHandling": true,
    "cursor.ai.codeGeneration.includeLogging": true,
    "cursor.ai.codeGeneration.includeTypeHints": true,
    "cursor.ai.codeGeneration.includeValidation": true,
    "cursor.ai.codeGeneration.includeSecurityChecks": true,
    "cursor.ai.codeGeneration.targetCoverage": 95,
  
    "// ==========================================": "TAB COMPLETION (Quick Fixes)",
    "// ==========================================": "",
  
    "cursor.ai.tab.enabled": true,
    "cursor.ai.tab.model": "grok-code",
    "cursor.ai.tab.temperature": 0.2,
    "cursor.ai.tab.showSuggestions": true,
    "cursor.ai.tab.acceptSuggestionOnTab": true,
    "cursor.ai.tab.debounceDelay": 100,
    "cursor.ai.tab.maxSuggestionLength": 500,
  
    "// ==========================================": "COMPOSER 1 CONFIGURATION (Multi-File Work)",
    "// ==========================================": "",
  
    "cursor.composer.enabled": true,
    "cursor.composer.model": "opus-4-5",
    "cursor.composer.temperature": 0.3,
    "cursor.composer.maxTokens": 8000,
    "cursor.composer.streaming": true,
    "cursor.composer.enableAutoSave": true,
    "cursor.composer.autoSaveInterval": 30000,
    "cursor.composer.maxOpenBuffers": 10,
    "cursor.composer.includeTests": true,
    "cursor.composer.enableRealTimePreview": true,
    "cursor.composer.fallbackModel": "gpt-5-1-codex",
  
    "// ==========================================": "CHAT & CONTEXT MANAGEMENT",
    "// ==========================================": "",
  
    "cursor.chat.enabled": true,
    "cursor.chat.model": "claude-4-5",
    "cursor.chat.temperature": 0.3,
    "cursor.chat.maxContextMessages": 20,
    "cursor.chat.contextRetention": "smart",
    "cursor.chat.enableConversationPersistence": true,
    "cursor.chat.autoSummarizeAfter": 15,
    "cursor.chat.enableFileContext": true,
    "cursor.chat.enableCodebaseContext": true,
  
    "// ==========================================": "CODEBASE INDEXING & CONTEXT",
    "// ==========================================": "",
  
    "cursor.indexing.codebaseIndexing": true,
    "cursor.indexing.maxFileSize": 100000,
    "cursor.indexing.refreshInterval": 5000,
    "cursor.indexing.enableIncrementalIndexing": true,
    "cursor.indexing.enableSemanticIndexing": true,
    
    "cursor.indexing.excludePatterns": [
      "**/__pycache__/**",
      "**/.pytest_cache/**",
      "**/venv/**",
      "**/.venv/**",
      "**/node_modules/**",
      "**/.git/**",
      "**/build/**",
      "**/dist/**",
      "**/htmlcov/**",
      "**/.coverage/**",
      "**/eggs/**",
      "**/*.egg-info/**",
      "**/.mypy_cache/**",
      "**/.dmypy.json",
      "**/dmypy.json",
      "**/.hypothesis/**",
      "**/.tox/**",
      "**/.nox/**",
      "**/coverage.xml",
      "**/site-packages/**",
      "**/.idea/**",
      "**/.vscode/**",
      "**/.DS_Store",
      "**/*.pyc",
      "**/*.pyo",
      "**/*.pyd",
      "**/__pycache__"
    ],
  
    "cursor.indexing.includePatterns": [
      "**/*.py",
      "**/*.md",
      "**/*.json",
      "**/*.yaml",
      "**/*.yml",
      "**/*.toml",
      "**/requirements.txt",
      "**/pyproject.toml",
      "**/*.txt"
    ],
  
    "cursor.indexing.prioritizePatterns": [
      "arc_saga/**/*.py",
      "tests/**/*.py",
      "docs/**/*.md",
      ".cursorrules"
    ],
  
    "// ==========================================": "CODE QUALITY & FORMATTING",
    "// ==========================================": "",
  
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.fixAll.eslint": "explicit",
      "source.organizeImports": "explicit",
      "source.sortImports": "explicit",
      "source.removeUnusedImports": "explicit"
    },
  
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
      "--line-length=88",
      "--target-version=py311"
    ],
  
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
      "--max-line-length=88",
      "--disable=C0111",
      "--disable=R0903"
    ],
  
    "python.linting.mypyEnabled": true,
    "python.linting.mypyArgs": [
      "--strict",
      "--warn-return-any",
      "--warn-unused-configs",
      "--disallow-untyped-defs",
      "--python-version=3.11"
    ],
  
    "[python]": {
      "editor.defaultFormatter": "ms-python.python",
      "editor.formatOnSave": true,
      "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
      }
    },
  
    "// ==========================================": "ERROR DETECTION & DIAGNOSTICS",
    "// ==========================================": "",
  
    "problems.decorations.enabled": true,
    "problems.showCurrentInStatus": true,
    "editor.renderLineHighlight": "all",
    "editor.showUnused": true,
    "editor.unusedFolding": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
  
    "// ==========================================": "PERFORMANCE & OPTIMIZATION",
    "// ==========================================": "",
  
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 100,
    "editor.renderWhitespace": "selection",
    "editor.renderControlCharacters": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
  
    "// ==========================================": "MODEL FALLBACK STRATEGY",
    "// ==========================================": "",
  
    "cursor.modelFallback.enabled": true,
    "cursor.modelFallback.strategy": "intelligent",
    "cursor.modelFallback.primaryModel": "claude-4-5",
    "cursor.modelFallback.secondaryModel": "gpt-5-1-codex",
    "cursor.modelFallback.tertiaryModel": "opus-4-5",
    "cursor.modelFallback.rateLimitThreshold": 0.8,
    "cursor.modelFallback.switchOnLatency": true,
    "cursor.modelFallback.latencyThreshold": 5000,
  
    "// ==========================================": "COST OPTIMIZATION SETTINGS",
    "// ==========================================": "",
  
    "cursor.costControl.enabled": true,
    "cursor.costControl.dailyBudgetAlertThreshold": 0.8,
    "cursor.costControl.trackUsage": true,
    "cursor.costControl.showTokenCounts": true,
    "cursor.costControl.logUsageMetrics": true,
  
    "cursor.dailyDriver.enabled": true,
    "cursor.dailyDriver.model": "opus-4-5",
    "cursor.dailyDriver.temperature": 0.3,
    "cursor.dailyDriver.description": "Optimal cost-performance for routine work. Same token price as Sonnet 4.5 with superior reasoning.",
  
    "cursor.quickFix.enabled": true,
    "cursor.quickFix.model": "grok-code",
    "cursor.quickFix.temperature": 0.2,
    "cursor.quickFix.maxTokens": 2000,
    "cursor.quickFix.description": "Ultra-fast, cheapest option for syntax fixes and simple refactoring.",
  
    "cursor.complexTask.enabled": true,
    "cursor.complexTask.model": "claude-4-5",
    "cursor.complexTask.temperature": 0.3,
    "cursor.complexTask.maxTokens": 8000,
    "cursor.complexTask.description": "Premium model for architecture, complex multi-file changes, and ARC SAGA work.",
  
    "// ==========================================": "TEMPERATURE STRATEGY",
    "// ==========================================": "",
  
    "cursor.temperature.deterministic": 0.2,
    "cursor.temperature.balanced": 0.3,
    "cursor.temperature.creative": 0.7,
    "cursor.temperature.brainstorm": 0.9,
  
    "// ==========================================": "CONTEXT & MEMORY OPTIMIZATION",
    "// ==========================================": "",
  
    "cursor.context.enableSmartSummarization": true,
    "cursor.context.summaryThreshold": 150000,
    "cursor.context.prioritizeRecent": true,
    "cursor.context.prioritizeErrors": true,
    "cursor.context.maxHistoryMessages": 20,
    "cursor.context.enableConversationSearch": true,
    "cursor.context.enableSemanticSearch": true,
  
    "// ==========================================": "SECURITY & VALIDATION",
    "// ==========================================": "",
  
    "cursor.security.enableInputValidation": true,
    "cursor.security.enableSecretScanning": true,
    "cursor.security.warnOnSecrets": true,
    "cursor.security.blockSecretsInGeneration": true,
    "cursor.security.enableDependencyChecks": true,
    "cursor.security.enableVulnerabilityScanning": true,
    "cursor.security.requireValidationOnUserInput": true,
  
    "// ==========================================": "LOGGING & OBSERVABILITY",
    "// ==========================================": "",
  
    "cursor.logging.enabled": true,
    "cursor.logging.level": "info",
    "cursor.logging.logRequests": true,
    "cursor.logging.logResponses": false,
    "cursor.logging.logTokenUsage": true,
    "cursor.logging.logLatency": true,
    "cursor.logging.logErrors": true,
    "cursor.logging.outputFile": "./logs/cursor-usage.log",
  
    "cursor.metrics.enabled": true,
    "cursor.metrics.trackLatency": true,
    "cursor.metrics.trackAccuracy": true,
    "cursor.metrics.trackTokenEfficiency": true,
    "cursor.metrics.trackCosts": true,
  
    "// ==========================================": "ADVANCED FEATURES",
    "// ==========================================": "",
  
    "cursor.agents.enabled": true,
    "cursor.agents.enableAutoFix": true,
    "cursor.agents.enableAutoOptimize": true,
    "cursor.agents.enableAutoTest": true,
    "cursor.agents.autoFixThreshold": 0.8,
  
    "cursor.preview.enabled": true,
    "cursor.preview.showDiffPreview": true,
    "cursor.preview.enableLivePreview": true,
  
    "// ==========================================": "QUALITY GATES",
    "// ==========================================": "",
  
    "cursor.quality.enforceTypeHints": true,
    "cursor.quality.enforceDocstrings": true,
    "cursor.quality.enforceTests": true,
    "cursor.quality.enforceErrorHandling": true,
    "cursor.quality.enforceLogging": true,
    "cursor.quality.enforceSecurityValidation": true,
    "cursor.quality.minimumCoverage": 90,
    "cursor.quality.maximumComplexity": 10,
    "cursor.quality.enforceImportOrganization": true,
  
    "// ==========================================": "DEBUGGING & DEVELOPMENT",
    "// ==========================================": "",
  
    "debug.console.fontSize": 13,
    "debug.console.lineHeight": 1.5,
    "debug.openExplorerOnEnd": false,
  
    "// ==========================================": "EDITOR EXPERIENCE",
    "// ==========================================": "",
  
    "editor.fontSize": 14,
    "editor.fontFamily": "'Fira Code', 'Consolas', monospace",
    "editor.fontLigatures": true,
    "editor.lineHeight": 1.6,
    "editor.letterSpacing": 0.5,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.trimAutoWhitespace": true,
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.autoIndent": "keep",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": false
    },
  
    "// ==========================================": "EXTENSIONS & INTEGRATIONS",
    "// ==========================================": "",
  
    "extensions.autoCheck": true,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": false,
  
    "// ==========================================": "WORKSPACE SETTINGS",
    "// ==========================================": "",
  
    "files.watcherExclude": {
      "**/.git/**": true,
      "**/__pycache__/**": true,
      "**/.pytest_cache/**": true,
      "**/venv/**": true,
      "**/.venv/**": true,
      "**/node_modules/**": true,
      "**/.mypy_cache/**": true,
      "**/.hypothesis/**": true,
      "**/.tox/**": true,
      "**/build/**": true,
      "**/dist/**": true,
      "**/.coverage/**": true,
      "**/htmlcov/**": true,
      "**/*.egg-info/**": true,
      "**/.idea/**": true
    },
  
    "files.exclude": {
      "**/__pycache__": true,
      "**/.pytest_cache": true,
      "**/.mypy_cache": true,
      "**/.hypothesis": true,
      "**/.git": true,
      "**/.vscode": false,
      "**/.venv": true,
      "**/venv": true,
      "**/.env": true,
      "**/*.egg-info": true
    },
  
    "search.exclude": {
      "**/node_modules": true,
      "**/venv": true,
      "**/.venv": true,
      "**/__pycache__": true,
      "**/.pytest_cache": true,
      "**/.mypy_cache": true,
      "**/.git": true,
      "**/build": true,
      "**/dist": true,
      "**/.coverage": true,
      "**/htmlcov": true
    }
  }
  